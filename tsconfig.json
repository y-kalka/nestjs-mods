{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "incremental": true,
    "paths": {
      "@nestjs-mods/jwt": ["libs/jwt/src/index.ts"],
      "@nestjs-mods/rate-limiter": ["libs/rate-limiter/src/index.ts"]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
